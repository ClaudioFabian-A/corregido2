<div>
    <h2>Formulario para agregar productos</h2>

    <form action="./api/products" method="POST" id="idForm" required>
        <input type="text" id="title" placeholder="Title" required></br>
        <input type="string" id="description" placeholder="Description" required></br>
        <input type="number" id="price" placeholder="Price" required></br>
        <input type="string" id="thumbnail" placeholder="Thumbnail" required></br>
        <input type="string" id="code" placeholder="Code" required></br>
        <input type="number" id="stock" placeholder="Stock" required></br>
        <input type="string" id="category" placeholder="Category" required></br>
        <input type="boolean" id="status" placeholder="Status" required></br>
        <button type="submit" id="buttonSubmit">Agregar nuevo producto</button></br>
        <input type="number" id="IdProd" placeholder="Id de producto a eliminar"></br>
        <button type="button" id="buttonSubmitDelete">Eliminar el producto</button>

    </form>
</div>
<div id="prodContainer">aqui esta el div</div>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="./js/realTimeProducts.js"></script>